mysqladmin -u root -p create MarkDB
use MarkDB;
CREATE TABLE housing(
    ID BIGINT AUTO_INCREMENT,
    DATE DATE,
    PROPERTY_TYPE VARCHAR(30),
    ADDRESS VARCHAR(100),
    CITY VARCHAR(100),
    STATE VARCHAR(100),
    ZIP CHAR(5),
    PRICE MEDIUMINT,
    BEDS TINYINT,
    BATHS TINYINT,
    LOCATION VARCHAR(100),
    SQUARE_FEET SMALLINT,
    LOT_SIZE MEDIUMINT,
    YEAR_BUILT CHAR(4),
    DAYS_ON_MARKET SMALLINT,
    PRICE_PER_SQUARE_FOOT SMALLINT,
    HOA_PER_MONTH SMALLINT,
    SOURCE VARCHAR(100),
    LATITUDE FLOAT(9,6),
    LONGITUDE FLOAT(9,6),
    PRIMARY KEY ( ID )
);